{
    "name":         "qwesteo/job-queue-bundle",
    "type":         "symfony-bundle",
    "description":  "Allows to run and schedule Symfony console commands as background jobs. Support for PHP 8.1 and Symfony 6.4.",
    "keywords":     ["job", "queue", "background", "cron", "scheduled"],
    "homepage":     "https://jmsyst.com/bundles/JMSJobQueueBundle",
    "license":      "Apache-2.0",
    "authors": [
        {
            "name":   "Johannes M. Schmitt",
            "email":  "schmittjoh@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/common": "^3.2",
        "symfony/error-handler": "^6.4",
        "symfony/framework-bundle": "^6.4",
        "symfony/process": "^6.4"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.5",
        "doctrine/orm": "^3.1",
        "phpunit/phpunit": "^10.5",
        "symfony/browser-kit": "^6.4",
        "symfony/css-selector": "^6.4",
        "symfony/finder": "^6.4",
        "symfony/form": "^6.4",
        "symfony/intl": "^6.4",
        "symfony/twig-bundle": "^6.4",
        "symfony/validator": "^6.4",
        "symfony/yaml": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "JMS\\JobQueueBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JMS\\JobQueueBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
